public class Car {
    public static void main(String[] args){
        Car car = new Car();
        String brand = car.brand("Honda");
        String color = car.color("Red");
        String model = car.model("Civic");
        int number_of_wheels = car.number_of_wheels(4);
        int number_of_seats = car.number_of_seats(5);
        int max_speed = car.max_speed(180);
        String engine = car.engine("Honda Engine");
        System.out.println("The brand of the car is "+brand);
        System.out.println("The color of the car is "+color);
        System.out.println("The model of the car is "+model);
        System.out.println("The car has a " +number_of_wheels);
        System.out.println("The car has "+number_of_seats);
        System.out.println("The car has a speed of "+max_speed);
        System.out.println("The car engine is "+engine);


    }
    String brand(String brand){
        return brand;
    }
    String color(String color){
        return color;
    }
    String model(String model){
        return model;
    }
    int number_of_wheels(int number_of_wheels){
        return number_of_wheels;
    }
    int number_of_seats(int number_of_seats){
        return number_of_seats;
    }
    int max_speed(int max_speed){
        return max_speed;
    }
    String engine(String engine){
        return engine;
    }
}
