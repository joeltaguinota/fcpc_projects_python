public class Motorcycle {
    public static void main(String[] args){
        Motorcycle motorcycle = new Motorcycle();
        String brand = motorcycle.brand("Yamaha");
        String color = motorcycle.color("Black");
        String model = motorcycle.model("Mio Sporty");
        int number_of_wheels = motorcycle.number_of_wheels(2);
        int number_of_seats = motorcycle.number_of_seats(2);
        int max_speed = motorcycle.max_speed(113);
        String engine = motorcycle.engine("Yamaha Engine");
        System.out.println("The brand of the motorcycle is "+brand);
        System.out.println("The color of the motorcycle is "+color);
        System.out.println("The model of the motorcycle is "+model);
        System.out.println("The motorcycle has a "+number_of_wheels);
        System.out.println("The motorcycle has "+number_of_seats);
        System.out.println("The motorcycle has a speed of "+max_speed);
        System.out.println("The motorcycle engine is "+engine);

    }
    String brand(String brand){
        return brand;
    }
    String color(String color){
        return color;
    }
    String model(String model){
        return model;
    }
    int number_of_wheels(int number_of_wheels){
        return number_of_wheels;
    }
    int number_of_seats(int number_of_seats){
        return number_of_seats;
    }
    int max_speed(int max_speed){
        return max_speed;
    }
    String engine(String engine){
        return engine;
    }
}
